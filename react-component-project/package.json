{
  "name": "markdown-todos-manager",
  "version": "0.0.0",
  "description": "todos / issues with markdown",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest --verbose",
    "ups": "./node_modules/.bin/jest -u",
    "lint": "./node_modules/.bin/eslint app",
    "start": "webpack-dev-server --open --port 9000 --mode development --devtool inline-source-map",
    "watch-dev": "./node_modules/.bin/webpack -d --mode development --watch --devtool inline-source-map",
    "build-dev": "./node_modules/.bin/webpack -d  --mode development --devtool inline-source-map",
    "build-prod": "rm -f dist/*.js dist/*.css dist/*.html dist/*.map && rm -rf dist/assets && ./node_modules/.bin/webpack -p --mode production"
  },
  "author": "riverfr0zen@elitemail.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://irfanbaig.com/not-real"
  },
  "dependencies": {
    "auto-bind": "^2.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^2.1.1",
    "csslint": "^1.0.5",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
}
